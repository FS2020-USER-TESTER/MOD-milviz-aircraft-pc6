					<Component ID="KI525A_CDI_BAR">
						<Parameters Type="Default">
							<NODE_ID>KI525A_ANIM_Deviation_Bar</NODE_ID>
						</Parameters>
						<Component ID="#NODE_ID#" Node="#NODE_ID#">
