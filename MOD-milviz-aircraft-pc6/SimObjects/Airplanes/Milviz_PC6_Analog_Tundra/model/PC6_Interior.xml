									<ANIM_CODE>
										(A:GPS DRIVES NAV1, Bool) if{
											(A:GPS IS ACTIVE FLIGHT PLAN, Bool) if{
												#BEARING_CHECK# 90 &gt; if{ 
													1 sp0 
												}											
											} els{
												0 sp0
											}
										} els{
											(A:NAV TOFROM:1, Enum) 2 == if{
												1 sp0
											} els{
												0 sp0
											}
										}
										l0 10 *
									</ANIM_CODE>
									<FAILURE>#ELECTRICAL_CODE#</FAILURE>
									<FAILURE_CODE>0</FAILURE_CODE>								
									<ANIM_LAG>50</ANIM_LAG>
