						<BEARING_CHECK>
							(A:GPS WP TRUE BEARING, Degrees) (A:GPS WP TRUE REQ HDG, Degrees) - abs	s1 180 &gt; if{ 360 l1 - abs sp1 } l1
						</BEARING_CHECK>
					</Parameters>
					<UseTemplate Name="ASOBO_INSTRUMENT_CDI_Template">
						<ID>2</ID>
						<CIRCUIT_ID>10</CIRCUIT_ID>
