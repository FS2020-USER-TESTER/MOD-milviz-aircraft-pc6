		<!-- INSTRUMENTS ############################### -->
			<Component ID="INSTRUMENT_PrimaryInstruments">

				<Component ID="Analog_AirspeedIndicator">
					<UseTemplate Name="ASOBO_INSTRUMENT_Needle_Airspeed_Template">
						<ID>2</ID>
						<MIN_VALUE>10</MIN_VALUE>
						<MAX_VALUE>210</MAX_VALUE>
					</UseTemplate>
				</Component>

				<Component ID="Analog_AttitudeIndicator">
					<UseTemplate Name="ASOBO_INSTRUMENT_AttitudeIndicator_Template">
						<ID>2</ID>
						<USE_SUCTION_PRESSURE_CHECK/>
						<NO_CAGE>True</NO_CAGE>
						<PITCH_MIN_VALUE>-20</PITCH_MIN_VALUE>
						<PITCH_MAX_VALUE>20</PITCH_MAX_VALUE>
						<ANIM_VALUE_PLUS_REF_DEGREES>1</ANIM_VALUE_PLUS_REF_DEGREES> <!-- Frame where pitch equals 20° -->
						<ANIM_VALUE_MINUS_REF_DEGREES>99</ANIM_VALUE_MINUS_REF_DEGREES> <!-- Frame where pitch equals -20° -->
						<ANIM_PITCH_LENGTH>100</ANIM_PITCH_LENGTH><!-- Frame count -->
						<ANIM_PITCH_0>50</ANIM_PITCH_0> <!-- Frame where pitch equals 0° -->
						<KNOB_HIGHLIGHT_NODE_ID>INSTRUMENT_AttitudeIndicator_Knob_2</KNOB_HIGHLIGHT_NODE_ID>
					</UseTemplate>
				</Component>

				<Component ID="Analog_Altimeter_1">

					<UseTemplate Name="ASOBO_INSTRUMENT_Altimeter_Template">
						<ID>1</ID>
						<KNOB_HIGHLIGHT_NODE_ID>INSTRUMENT_Knob_Altimeter_1</KNOB_HIGHLIGHT_NODE_ID>
					</UseTemplate>

					<!-- baro hundredths -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Baro_0001</ANIM_NAME>
						<ANIM_CODE>
							(A:KOHLSMAN SETTING HG:1, inHg) 100 * near 10 % 10 *
						</ANIM_CODE>
					</UseTemplate>
					<!-- baro tenths -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Baro_0010</ANIM_NAME>
						<ANIM_CODE>
							(A:KOHLSMAN SETTING HG:1, inHg) sp0
							l0 100 * near 10 % 10 * 0 == if{
								l0 10 * near 10 % 10 *
							} els{
								l0 10 * int 10 % 10 *
							}
						</ANIM_CODE>
					</UseTemplate>
					<!-- baro 1's -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Baro_0100</ANIM_NAME>
						<ANIM_CODE>
							(A:KOHLSMAN SETTING HG:1, inHg) sp0
							l0 100 * near 10 % 10 * 0 == l0 10 * near 10 % 10 * 0 == and if{
								l0 near 10 % 10 *
							} els{
								l0 int 10 % 10 *
							}
						</ANIM_CODE>
					</UseTemplate>
					<!-- baro 10's -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Baro_1000</ANIM_NAME>
						<ANIM_CODE>
							(A:KOHLSMAN SETTING HG:1, inHg) sp0
							l0 100 * near 10 % 10 * 0 == l0 10 * near 10 % 10 * 0 == l0 near 10 % 10 * 0 == and and if{
								l0 10 / near 10 % 10 *
							} els{
								l0 10 / int 10 % 10 *
							}
						</ANIM_CODE>
					</UseTemplate>

					<!-- 100's -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Altimeter_100_1</ANIM_NAME>
						<ANIM_CODE>
							(A:INDICATED ALTITUDE:1, feet) 0 &gt;=
							if{ (A:INDICATED ALTITUDE:1, feet) 100 / 10 % 10 * }
							els{ (A:INDICATED ALTITUDE:1, feet) 100 / 10 % 10 * 100 + }
						</ANIM_CODE>						
					</UseTemplate>
					<!-- 1000's -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Altimeter_1000_1</ANIM_NAME>
						<ANIM_CODE>
							(A:INDICATED ALTITUDE:1, feet) 0 &gt;=
							if{ (A:INDICATED ALTITUDE:1, feet) 1000 / 10 % 10 * 4 * }
							els{ (A:INDICATED ALTITUDE:1, feet) 1000 / 10 % 10 * 100 + 4 * }
						</ANIM_CODE>
						<ANIM_LENGTH>400</ANIM_LENGTH>
					</UseTemplate>
					<!-- 10000's -->
					<UseTemplate Name="ASOBO_GT_Anim_Code">
						<ANIM_NAME>INSTRUMENT_Drum_Altimeter_10000_1</ANIM_NAME>
						<ANIM_CODE>
							(A:INDICATED ALTITUDE:1, feet) 0 &gt;=
							if{ (A:INDICATED ALTITUDE:1, feet) 10000 / flr 10 * }
							els{ (A:INDICATED ALTITUDE:1, feet) 10000 / flr 10 * 100 + }
						</ANIM_CODE>
					</UseTemplate>

				</Component>

				<Component ID="Analog_VSI">
