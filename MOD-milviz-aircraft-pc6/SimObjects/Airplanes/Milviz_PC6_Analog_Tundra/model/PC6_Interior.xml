					<Component ID="TF_FLAGS">
						<Parameters Type="Default">
							<BEARING_CHECK>
								(A:GPS WP TRUE BEARING, Degrees) (A:GPS WP TRUE REQ HDG, Degrees) - abs	s1 180 &gt; if{ 360 l1 - abs sp1 } l1
							</BEARING_CHECK>
						</Parameters>

						<Component ID="KI525A_TO_FLAG">
							<Parameters Type="Default">
								<NODE_ID>KI525A_ANIM_To_Flag</NODE_ID>
							</Parameters>
							<Component ID="#NODE_ID#" Node="#NODE_ID#">
								<UseTemplate Name="ASOBO_GT_Anim">
									<ANIM_NAME>#NODE_ID#</ANIM_NAME>
									<ANIM_CODE>
										(A:GPS DRIVES NAV1, Bool) if{
											(A:GPS IS ACTIVE FLIGHT PLAN, Bool) if{
												#BEARING_CHECK# 90 &lt; if{ 
													1 sp0 
												}											
											} els{
												0 sp0
											}
										} els{
											(A:NAV TOFROM:1, Enum) 1 == if{
												1 sp0
											} els{
												0 sp0
											}
										}
										l0 10 *
									</ANIM_CODE>
									<FAILURE>#ELECTRICAL_CODE#</FAILURE>
									<FAILURE_CODE>0</FAILURE_CODE>
									<ANIM_LAG>50</ANIM_LAG>
									<ANIM_LENGTH>10</ANIM_LENGTH>
								</UseTemplate>						
							</Component>
						</Component>

