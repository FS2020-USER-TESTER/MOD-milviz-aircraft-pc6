					<Component ID="KI525A_GLIDESLOPE_VISIBILITY">
						<Parameters Type="Default">
							<NODE_ID>KI525A_ANIM_Glideslope_Visibility</NODE_ID>
							<GLIDESLOPE_CODE>
								(A:GPS DRIVES NAV1, Bool) if{
									(A:GPS HAS GLIDEPATH, Bool)
								} els{
									(A:NAV HAS GLIDE SLOPE:1, Bool)
								}							
							</GLIDESLOPE_CODE>
