						<IS_AVIONICS_ON>#ELECTRICAL_CODE#</IS_AVIONICS_ON>
						<WWISE_EVENT>rotary_cdi_obs</WWISE_EVENT>
						<COUNT>4</COUNT>
						<FLAG_TO_CODE>
							(L:AS430_CDI_Source_1, Bool) if{
								(A:GPS IS ACTIVE FLIGHT PLAN, Bool) if{
									#BEARING_CHECK# 90 &lt; if{ 
										1 sp0 
									}											
								} els{
									0 sp0
								}
							} els{
								(A:NAV TOFROM:2, Enum) 1 == if{
									1 sp0
								} els{
									0 sp0
								}
							}
							l0
						</FLAG_TO_CODE>
						<FLAG_FROM_CODE>
							(L:AS430_CDI_Source_1, Bool) if{
								(A:GPS IS ACTIVE FLIGHT PLAN, Bool) if{
									#BEARING_CHECK# 90 &gt; if{ 
										1 sp0 
									}											
								} els{
									0 sp0
								}
							} els{
								(A:NAV TOFROM:2, Enum) 2 == if{
									1 sp0
								} els{
									0 sp0
								}
							}
							l0
						</FLAG_FROM_CODE>
						<FLAG_GLIDE_CODE>
							(L:AS430_CDI_Source_1, Bool) if{
								(A:GPS HAS GLIDEPATH, Bool)
							} els{
								(A:NAV HAS GLIDE SLOPE:2, Bool)
							}
						</FLAG_GLIDE_CODE>						
					</UseTemplate>
				</Component>

