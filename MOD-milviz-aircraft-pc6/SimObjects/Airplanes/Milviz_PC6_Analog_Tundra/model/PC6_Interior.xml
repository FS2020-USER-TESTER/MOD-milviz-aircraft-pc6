								<ANIM_LAG>50</ANIM_LAG>
								<ANIM_LENGTH>100</ANIM_LENGTH>
							</UseTemplate>
							<UseTemplate Name="ASOBO_GT_Visibility_Code">
								<VISIBILITY_CODE>
									#GLIDESLOPE_CODE#
								</VISIBILITY_CODE>
							</UseTemplate>
						</Component>
